    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui-comp": "vitest run --config vitest.config.ui.ts",
    "test:ui-comp:watch": "vitest --config vitest.config.ui.ts",
    "test:ui-e2e": "playwright test",
    "test:ui-e2e:watch": "playwright test --ui",
    "test:ui-e2e:mcp": "echo '\\n  Run UI E2E in browser via Playwright MCP:\\n  1. Start app: npm run dev\\n  2. In Cursor: run command \"test-ui-browser\" or ask: Run UI tests in browser\\n  Flows: tests/e2e/ui/ (home, expenses, bills, settings, analytics, integration)\\n'",
    "test:all": "npm run test:run && npm run test:ui-comp",
    "test:ui-all": "npm run test:ui-comp && npm run test:ui-e2e",
    "test:ui:watch": "vitest --config vitest.config.ui.ts"
  },
